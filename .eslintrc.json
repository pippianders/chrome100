{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": "off",
    "quotes": "off",
    "object-curly-spacing": "off",
    "comma-dangle": "off",
    "guard-for-in": "off"
  }
}
